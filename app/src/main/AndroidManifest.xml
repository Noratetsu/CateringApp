<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="group6.cateringapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".system_home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".selected_event_screen" />
        <activity android:name=".user_request_summary" />
        <activity android:name=".caterer_home" />
        <activity android:name=".user_homepage" />
        <activity android:name=".login_screen" />
        <activity android:name=".registration_form" />
        <activity android:name=".staff_homepage" />
        <activity
            android:name=".ViewAvailableStaff"
            android:label="@string/title_activity_view_available_staff"
            android:parentActivityName=".selected_event_screen"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="group6.cateringapp.selected_event_screen" />
        </activity>
        <activity android:name=".add_resources" />
        <activity android:name=".request_event_1" />
        <activity android:name=".event_summary_screen"></activity>
    </application>

</manifest>